<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="Hibernate_maventry.HQL.GoalAlert_HQL" table="GOALALERT">
		<id name="id" type="int">
			<column name="ID" />
			<generator class="increment" />
		</id>
		<property name="message" type="java.lang.String">
			<column name="MESSAGE" />
		</property>
	</class>
	<query name="AllGoalAlerts">
		<![CDATA[select goalalert_hql from GoalAlert_HQL as goalalert_hql]]>
	</query>
</hibernate-mapping>
