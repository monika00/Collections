<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="Hibernate_maventry.HQL.ProteinData_HQL" table="PROTEINDATA">
		<id name="id" type="int">
			<column name="ID" />
			<generator class="increment">
				<param name="property">user</param>
			</generator>
		</id>
		
		<one-to-one name="user" class="Hibernate_maventry.HQL.User_HQL" property-ref="proteinData" />
		
		<property name="total" type="int">
			<column name="TOTAL" />
		</property>
		<property name="goal" type="int">
			<column name="GOAL" />
		</property>
		
	</class>
</hibernate-mapping>
