<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="Hibernate_maventry.HB.User" table="USERS">
		<id name="id" type="int">
			<column name="ID" />
			<generator class="increment" />
		</id>
		<property name="name" type="java.lang.String">
			<column name="NAME" />
		</property>
		<component name="ProteinData">
			<property name="total" type="int">
				<column name="TOTAL" />
			</property>
			<property name="goal" type="int">
				<column name="GOAL" />
			</property>
		</component>
		
		<!-- <set name="history" table="USER_HISTORY">
			<key column="ID"/>
			<composite-element class="Hibernate_maventry.HB.UserHistory">
				<property name="entryTime" type="date" column="ENTRY_TIME" />
				<property name="entry" type="string" column="ENTRY" />
			</composite-element>
		</set> -->
		
		<!-- <list name="history" table="USER_HISTORY">
			<key column="ID"/>
			<list-index column="POSITION"/>
			<composite-element class="Hibernate_maventry.HB.UserHistory">
				<property name="entryTime" type="date" column="ENTRY_TIME" />
				<property name="entry" type="string" column="ENTRY" />
			</composite-element>
		</list> -->
		
		<!-- <map name="history" table="USER_HISTORY">
			<key column="ID"/>
			<map-key type="string" column="ENTRY_IDENTIFIER" />
			<composite-element class="Hibernate_maventry.HB.UserHistory">
				<property name="entryTime" type="date" column="ENTRY_TIME" />
				<property name="entry" type="string" column="ENTRY" />
			</composite-element>
		</map> -->
		
		<idbag name="history" table="USER_HISTORY">
			<collection-id type="int" column="ID">
				<generator class="increment" />
			</collection-id>
			<key column="USER_ID"/>
			<composite-element class="Hibernate_maventry.HB.UserHistory">
				<property name="entryTime" type="date" column="ENTRY_TIME" />
				<property name="entry" type="string" column="ENTRY" />
			</composite-element>
		</idbag>
		
	</class>
</hibernate-mapping>
